<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jrlandau.robotcmdr" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Robot Commander</name>
    <description>
        Allows you to control the UR robot with touch or voice commands.
    </description>
    <author email="jrlandau@gmail.com" href="https://ubiquityrobotics.com/">
        Ubiquity Robotics Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <icon density="hdpi" src="res/android/hdpi.png" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-tts" spec="~0.2.3" />
    <plugin name="cordova-plugin-speechrecognition" spec="~1.1.2" />
    <plugin name="cordova.plugins.diagnostic" spec="~3.5.0" />
    <plugin name="com.koalasafe.cordova.plugin.multicastdns" spec="https://github.com/stevenpack/phonegap-multicastdns" />
</widget>
